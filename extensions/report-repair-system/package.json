{
  "name": "report-repair-system",
  "description": "Please enter a description for your extension",
  "icon": "extension",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-bundle"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "hook",
        "name": "product-movements-hook",
        "source": "src/product-movements-hook/index.js"
      },
      {
        "type": "hook",
        "name": "auto-generate-documents-number",
        "source": "src/auto-generate-documents-number/index.js"
      },
      {
        "type": "interface",
        "name": "button-export-file",
        "source": "src/button-export-file/index.js"
      }
    ],
    "host": "^10.10.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "validate": "directus-extension validate",
    "add": "directus-extension add"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "16.0.1",
    "vue": "^3.5.21"
  },
  "dependencies": {
    "vue-router": "^4.5.1"
  }
}
